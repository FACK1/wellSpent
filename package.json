  "bugs": {
    "url": "https://github.com/FACK1/wellSpent/issues"
  },
  "homepage": "https://github.com/FACK1/wellSpent#readme",
  "dependencies": {
    "concurrently": "^4.1.0",
    "env2": "^2.2.2",
    "express": "^4.16.4",
    "i": "^0.3.6",
    "nodemon": "^1.18.10",
    "path": "^0.12.7"
  }
}
